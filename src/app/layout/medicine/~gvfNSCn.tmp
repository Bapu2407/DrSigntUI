.profile-image {
    width: 220px;
    height: 220px;
    border-radius: 50%;
    border: 1px solid #ccc;
    cursor: pointer;
    display: inline-block;
    position: relative;

    background-repeat: no-repeat;
    background-size: cover;
    background-position: center;

    .edit-icon {
      background: white;
      height: 25px;
      display: inline-block;
      width: 25px;
      border-radius: 20%;
      padding: 4px;
      margin-top:-10px;
      position: absolute;
      bottom: 0;
      right: 0;
      padding-top:7px !important;
      padding-bottom:2px !important;

    }
  }
  .clearfix {
    position: relative;

    font-size: 14px;
  }
